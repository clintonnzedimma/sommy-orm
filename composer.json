{
  "name": "clintonnzedimma/sommy-orm",
  "description": "Sequelize-style lightweight ORM for PHP (Sommy ORM)",
  "type": "library",
  "keywords": [
    "orm",
    "pdo",
    "mysql",
    "mariadb",
    "postgresql",
    "sqlite",
    "migrations"
  ],
  "require": {
    "php": ">=8.0",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "Sommy\\ORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Database\\Models\\": "database/models/"
    }
  },
  "bin": [
    "bin/sommy"
  ]
}
